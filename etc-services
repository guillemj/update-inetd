xtel		1313/tcp			# french minitel
cfinger		2003/tcp			# GNU Finger
icp		3130/tcp			# Internet Cache Protocol (Squid)
icp		3130/udp			# Internet Cache Protocol (Squid)
postgres	4321/tcp			# POSTGRES
