address_family ipx
architecture linux

method dynamic
  options
    frame

  up
    ipx_interface add %iface% %frame%

  down
    ipx_interface del %iface% %frame%

method static
  options
    frame
    netnum

  up
    ipx_interface add %iface% %frame% %netnum%

  down
    ipx_interface del %iface% %frame%

